                <td>{{ user.en_attente }}</td>
            {% else %}
                <td>-</td>
            {% endif %}

            {% if user.gagne is defined %}
                <td>{{ user.gagne }}</td>
            {% else %}
                <td>-</td>
            {% endif %}

            {% if user.perdu is defined %}
                <td>{{ user.perdu }}</td>
            {% else %}
                <td>-</td>
            {% endif %}

            {% if user.inexploitable is defined %}
                <td>{{ user.inexploitable }}</td>
            {% else %}
                <td>-</td>
            {% endif %}

            {% if user.tx is defined %}
                <td>{{ user.tx }} %</td>
            {% else %}
                <td>-</td>
            {% endif %}

        </tr>

    {% endfor %}

</table>
